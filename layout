e(window?.DtGetDefaultConfig?.execute() ?? '{}');\n            const urlCheckUser = new URL(config?.url_check_user ?? 'http://0.0.0.0:5000');\n            urlCheckUser.protocol = 'ws:';\n            return new WebSocket(urlCheckUser);\n        };\n\n        const startActions = socket => {\n            socket.send(JSON.stringify({\n                action: 'all',\n                data: {}\n            }));\n        };\n\n        const startSocket = () => {\n            const socket = createConnection();\n            socket.onopen = e => startActions(socket);\n            socket.onmessage = e => parseResponseData(e.data);\n        };\n\n        btnHotSpot.addEventListener('click', toggleHotSpot);\n\n        const hotSpotStatus = window?.DtGetStatusHotSpotService?.execute() ?? 'STOPPED';\n        if (hotSpotStatus == 'RUNNING') {\n            iconHotSpotOff.style.display = 'none';\n            iconHotSpotOn.style.display = 'block';\n        } else {\n            iconHotSpotOn.style.display = 'none';\n            iconHotSpotOff.style.display = 'block';\n        }\n\n\n        startSocket();\n\n    </script>\n</body>\n\n</html>",
